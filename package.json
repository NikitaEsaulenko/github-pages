{
  "name": "@retailrocket-search/app",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.5.0",
  "publishConfig": {
    "registry": "https://nexus.internal.sailplay.org/repository/js-npm/"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./index.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "storybook:dev": "storybook dev -p 6006 --ci",
    "storybook:build": "storybook build -o ./dist-storybook",
    "test:vitest": "vitest",
    "test:storybook": "test-storybook",
    "test:storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"http-server ./dist-storybook --port 6006 --silent\" \"wait-on tcp:6006 && pnpm run test:storybook\"",
    "lint:code": "eslint ./**/*.{vue,ts,js} --fix",
    "lint:style": "stylelint ./**/*.{vue,css} --fix --ignore-path .gitignore",
    "lint:commit": "commitlint --edit",
    "format": "prettier ./**/*.{vue,ts,js,css} --write --ignore-path .gitignore",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "pre-commit": "pnpm lint:code & pnpm lint:style & pnpm format & pnpm test:vitest & pnpm test:storybook"
  },
  "dependencies": {
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/format": "^19.5.0",
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.14.0",
    "@release-it/conventional-changelog": "^9.0.3",
    "@retailrocket/retailrocket.analytics.rocketsearch.apiclients.v2": "^1.0.136-dev",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.4.3",
    "@storybook/addon-interactions": "^8.4.3",
    "@storybook/addon-links": "^8.4.3",
    "@storybook/addon-onboarding": "^8.4.3",
    "@storybook/blocks": "^8.4.3",
    "@storybook/test": "^8.4.3",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/types": "^8.4.3",
    "@storybook/vue3": "^8.4.3",
    "@storybook/vue3-vite": "^8.4.3",
    "@thames/monads": "^0.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-vue": "^9.31.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "lodash-es": "^4.17.21",
    "npm-run-all2": "^6.2.6",
    "playwright": "1.48.1",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0",
    "sass-embedded": "^1.81.0",
    "storybook": "^8.4.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10",
    "wait-on": "^8.0.1"
  }
}
